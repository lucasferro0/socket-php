<?php

require 'vendor/autoload.php';

use SocketPhp\Commands\CommandManager;

$commandManager = new CommandManager($argv);

$commandManager->registerCommand('file:store', 'SocketPhp\Commands\FileCommand@store');

$commandManager->registerCommand('file:get', 'SocketPhp\Commands\FileCommand@get');

$commandManager->registerCommand('file:destroy', 'SocketPhp\Commands\FileCommand@destroy');

$commandManager->registerCommand('folder:make', 'SocketPhp\Commands\FileCommand@make');

$commandManager->registerCommand('folder:scan', 'SocketPhp\Commands\FileCommand@scan');

$commandManager->registerCommand('folder:delete', 'SocketPhp\Commands\FileCommand@delete');